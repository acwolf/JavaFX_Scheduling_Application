<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportController">
   <children>
      <Label layoutX="23.0" layoutY="23.0" text="Report: Unscheduled Customers">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
       <TableView fx:id="customers_table" layoutX="23.0" layoutY="54.0" prefHeight="240.0" prefWidth="546.0">
           <columns>
               <TableColumn fx:id="tbl_customer_id" prefWidth="75.0" text="Customer ID" />
               <TableColumn fx:id="tbl_customer_name" prefWidth="75.0" text="Name" />
               <TableColumn fx:id="tbl_customer_address" prefWidth="121.0" text="Address" />
               <TableColumn fx:id="tbl_customer_postal_code" prefWidth="74.0" text="Postal Code" />
               <TableColumn fx:id="tbl_customer_phone" prefWidth="106.0" text="Phone" />
               <TableColumn fx:id="tbl_customer_division_id" prefWidth="71.0" text="Division" />
           </columns>
       </TableView>

       <Button layoutX="490.0" layoutY="340.0" mnemonicParsing="false" onAction="#goto_dashboard" text="Dashboard" />
   </children>
</AnchorPane>
